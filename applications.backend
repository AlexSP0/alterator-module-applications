[Alterator Entry]
Type = Backend
Module = executor
Name = global
Interface = applications

[Info]
execute = sh /usr/lib/alterator-module-applications/alterator/app-info {name}
stdout_bytes = enabled
action_id = Info

[List]
execute = sh /usr/lib/alterator-module-applications/alterator/list-apps
stdout_strings = enabled
action_id = List
